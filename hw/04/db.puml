@startuml
entity "member" as member {
 * ID: number
 --
 * first_name: varchar2(128)
 * last_name: varchar2(128)
 * date_joined: date
}

entity "email" as email {
 * MEMBER_ID: number
 * ADDRESS: varchar2(128)
}

entity "seminar" as seminar {
 * ID: number
 --
